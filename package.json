    "eslint": "^8.18.0",
    "eslint-config-alloy": "^4.6.0",
    "eslint-plugin-react": "^7.30.1",
    "html-webpack-plugin": "4.5.2",
    "husky": "4.3.8",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.3",
    "jest-junit": "^14.0.0",
    "lint-staged": "10.5.4",
    "react-test-renderer": "16.14.0",
    "sass": "^1.53.0",
    "sass-loader": "^10",
    "style-loader": "^2",
    "svg-sprite-loader": "^6.0.11",
