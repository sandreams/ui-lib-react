    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@webpack-cli/serve": "^1.7.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.18.0",
    "eslint-config-alloy": "^4.6.0",
    "eslint-plugin-react": "^7.30.1",
    "html-webpack-plugin": "4.5.2",
    "husky": "4.3.8",
